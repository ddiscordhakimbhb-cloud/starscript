local function run(url)
    task.spawn(function()
        local success, err = pcall(function()
            loadstring(game:HttpGet(url, true))()
        end)

        if not success then
            warn("[Loader] Error in script:", url)
        end
    end)
end

-- Script 1
run("https://raw.githubusercontent.com/renardofficiel/game/refs/heads/main/MurderMystery2/main.lua")

-- Script 2
run("https://raw.githubusercontent.com/mm2scripthub/TravHub/refs/heads/main/travscript")

-- Script 3
run("https://pastefy.app/oE8SCnvJ/raw")
